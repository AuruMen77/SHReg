<AnchorPane id="AnchorPane" prefHeight="250.0" prefWidth="375.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SetStudentClearanceController">
    <VBox>
        <HBox prefHeight="100" prefWidth="375" alignment="CENTER">
            <children>
                <Label fx:id="lblGradeSubmission" style="-fx-font-size: 24;" text="Online Grade Submission"/>
            </children>
            <padding>
                <Insets left="5.0" top="5.0" />
            </padding>
        </HBox>
        <HBox prefHeight="25" prefWidth="375"  alignment="CENTER">
            <Label text="SY" textFill="#000000" />
            <ComboBox fx:id="cbSY" prefHeight="25.0" prefWidth="104.0"  HBox.hgrow="ALWAYS" >
                <HBox.margin>
                    <Insets right="10" />
                </HBox.margin>
            </ComboBox>
            <Label text="Sem" textFill="#000000" />
            <ComboBox fx:id="cbSem" prefHeight="25.0" prefWidth="104.0" />
        </HBox>
        <HBox prefHeight="25" prefWidth="375"  alignment="CENTER">
          
        </HBox>
        <HBox prefHeight="50" prefWidth="375" alignment="CENTER">
            <children>
                <Button fx:id="btnStudentClearance" mnemonicParsing="false" onAction="#actionSetStudentClearanceToggle" style="-fx-font-size: 18;" text="Open Grade Submission"/>
            </children>
            <padding>
                <Insets left="5.0" top="5.0" />
            </padding>
        </HBox>
       
    </VBox>
</AnchorPane>
